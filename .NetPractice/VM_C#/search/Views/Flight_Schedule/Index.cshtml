@model IEnumerable<search.Models.Flight_Schedule>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.FlightDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Departure)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Arrival)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AirCraft1.AcNumber)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AirFare.AfID)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.FlightDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Departure)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Arrival)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AirCraft1.AcNumber)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AirFare.AfID)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.FlID }) |
            @Html.ActionLink("Details", "Details", new { id=item.FlID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.FlID })
        </td>
    </tr>
}

</table>
